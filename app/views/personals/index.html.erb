<h2><%= link_to 'Home', personals_path %> </h2>
<table>
  <thead>
    <tr>
    
<th> <h3><%= link_to 'Aygcode', ascn_path %></h3></th>

      <th><h3><%= link_to 'Name', ascname_path %></th>
      <th>Dob</th>
      <th>Doj</th>
      <th>Contact1</th>
      <th>Contact2</th>
      <th>Email</th>
      <th>R address</th>
      <th>R area</th>
      <th>R city</th>
      <th>R state</th>
      <th>R pincode</th>
      <th>Profession</th>
      <th>Bloodgroup</th>
      <th>Pan</th>
      <th>Married</th>
      <th>Anniversary date</th>
      <th>Interest</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @personals.each do |personal| %>
      <tr>
        <td><%= link_to personal.aygcode, personal_path(personal) %></td>
	<td><%= link_to personal.name, personal_path(personal) %></td>
        
        <td><%= personal.dob %></td>
        <td><%= personal.doj %></td>
        <td><%= personal.contact1 %></td>
        <td><%= personal.contact2 %></td>
        <td><%= personal.email %></td>
        <td><%= personal.r_address %></td>
        <td><%= personal.r_area %></td>
        <td><%= personal.r_city %></td>
        <td><%= personal.r_state %></td>
        <td><%= personal.r_pincode %></td>
        <td><%= personal.profesion %></td>
        <td><%= personal.bloodgroup %></td>
        <td><%= personal.pan %></td>
        <td><%= personal.married %></td>
        <td><%= personal.anniversary_date %></td>
        <td><%= personal.interest %></td>
        <td><%= link_to 'Show', personal %></td>
        <td><%= link_to 'Edit', edit_personal_path(personal) %></td>
        <td><%= link_to 'Destroy', personal, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

